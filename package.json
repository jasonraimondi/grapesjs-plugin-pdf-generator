{
  "name": "grapesjs-plugin-pdf-generator",
  "version": "0.0.7",
  "description": "Grapesjs Plugin Pdf Generator",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jasonraimondi/grapesjs-plugin-pdf-generator.git"
  },
  "scripts": {
    "prepublish": "rm -rf dist && pnpm build",
    "start": "grapesjs-cli serve",
    "build": "grapesjs-cli build -- --public-path=''",
    "bump": "npm version patch -m 'Bump v%s'"
  },
  "keywords": [
    "grapesjs",
    "plugin"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "jspdf": "^2.5.1"
  },
  "devDependencies": {
    "grapesjs": "^0.18.4",
    "grapesjs-cli": "^3.0.0",
    "typescript": "^4.6.4"
  }
}
